{
  "compilerOptions": {
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strict": true,
    "jsx": "preserve",
    "lib": ["dom", "es2015"],
    "target": "es5"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
